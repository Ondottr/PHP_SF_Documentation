{
  "authors": [
    {
      "name": "Dmytro Dyvulskyi",
      "role": "CEO & Founder and Lead Developer of Nations Original",
      "email": "eklerk397@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "App\\View\\": "templates/",
      "Doctrine\\": "Platform/CustomRepositories/Doctrine"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "symfony/thanks": true
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.*"
    }
  },
  "license": "ISC",
  "minimum-stability": "stable",
  "name": "nations-original/php-simple-framework-template",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "8.1.*",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-http": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-memcached": "*",
    "ext-redis": "*",
    "ext-zlib": "*",
    "composer/package-versions-deprecated": "1.11.99.4",
    "phpdocumentor/reflection-docblock": "^5.3",
    "predis/predis": "^1.1",
    "psr/simple-cache": "^3.0",
    "symfony/asset": "6.*",
    "symfony/console": "6.*",
    "symfony/dotenv": "6.*",
    "symfony/expression-language": "6.*",
    "symfony/flex": "^1.17|^2",
    "symfony/form": "6.*",
    "symfony/framework-bundle": "6.*",
    "symfony/http-client": "6.*",
    "symfony/intl": "6.*",
    "symfony/mailer": "6.*",
    "symfony/mime": "6.*",
    "symfony/notifier": "6.*",
    "symfony/process": "6.*",
    "symfony/property-access": "6.*",
    "symfony/property-info": "6.*",
    "symfony/proxy-manager-bridge": "6.*",
    "symfony/runtime": "6.*",
    "symfony/serializer": "6.*",
    "symfony/string": "6.*",
    "symfony/validator": "6.*",
    "symfony/web-link": "6.*",
    "symfony/yaml": "6.*",
    "twig/extra-bundle": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "6.*",
    "symfony/css-selector": "6.*",
    "symfony/debug-bundle": "6.*",
    "symfony/maker-bundle": "^1.43",
    "symfony/phpunit-bridge": "^6.2",
    "symfony/stopwatch": "6.*",
    "symfony/thanks": "^1.2"
  },
  "scripts": {
    "auto-scripts": {
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "cache:clear": "symfony-cmd",
      "app:cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "post-autoload-dump": [
      "@auto-scripts"
    ]
  },
  "type": "project"
}
